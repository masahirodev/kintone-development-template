    "lint": "eslint src",
    "upload": "babel-node scripts/uploader.js",
    "init": "npx ts-node --files scripts/init.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
