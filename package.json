{
  "name": "kintone-development-template",
  "version": "0.1.0",
  "scripts": {
    "start": "babel-node scripts/start.js",
    "build": "babel-node scripts/build.js",
    "dev": "webpack --mode development --config-name %npm_config_webpackConfigName% --env APP_NAME=%npm_config_appName% --watch",
